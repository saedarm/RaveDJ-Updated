<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net8.0-windows10.0.26100.0</TargetFramework>
        <AssemblyName>raveDjDownloader</AssemblyName>
        <LangVersion>8</LangVersion>
        <Nullable>enable</Nullable>
        <PackageVersion>2.0.1</PackageVersion>
        <Authors>Loc</Authors>
        <RepositoryUrl>https://github.com/MiniCheese26/RaveDJ-Downloader</RepositoryUrl>
        <AssemblyVersion>2.0.1</AssemblyVersion>
        <FileVersion>2.0.1</FileVersion>
        <StartupObject>RaveDjDownloader.Program</StartupObject>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Goblinfactory.Konsole" Version="6.2.2" />
      <PackageReference Include="Newtonsoft.Json" Version="13.0.4" />
    </ItemGroup>

</Project>
